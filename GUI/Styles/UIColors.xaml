    <SolidColorBrush x:Key="color7" Color="#FA7070"/>
    <SolidColorBrush x:Key="color8" Color="#78A3FC"/>
    <SolidColorBrush x:Key="color9" Color="#07F3C0"/>
    <SolidColorBrush x:Key="color10" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="color11" Color="#000000"/>
    <!--Font Colors-->
    <SolidColorBrush x:Key="titleColor1" Color="#000000"/>
    <SolidColorBrush x:Key="titleColor2" Color="#0072BC"/>
    <SolidColorBrush x:Key="titleColor3" Color="#BD1515"/>
    <SolidColorBrush x:Key="titleColor4" Color="#FD5D5D"/>
    <SolidColorBrush x:Key="titleColor5" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="plainTextColor1" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="plainTextColor2" Color="#7C80C2"/>
    <SolidColorBrush x:Key="plainTextColor3" Color="#7376BD"/>
    <!--Panel Colors-->
    <SolidColorBrush x:Key="panelColor" Color="#200F53"/>
    <SolidColorBrush x:Key="panelOverColor" Color="#281269"/>
    <SolidColorBrush x:Key="panelActiveColor" Color="#FA7070"/>
