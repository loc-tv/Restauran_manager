    <Color x:Key="winBorderColor3">#BCCEF8</Color>
    <Color x:Key="winBorderColor4">#B1AFFF</Color>
    <Color x:Key="winBorderColor5">#98A8F8</Color>
    <Color x:Key="winBorderColor6">#000000</Color>
    <!--Color Palette-->
    <SolidColorBrush x:Key="color1" Color="#0072BC"/>
    <SolidColorBrush x:Key="color2" Color="#51A6DE"/>
    <SolidColorBrush x:Key="color3" Color="#4ADAEC"/>
    <SolidColorBrush x:Key="color4" Color="#D6D5A8"/>
    <SolidColorBrush x:Key="color5" Color="#7E82FC"/>
